{
  "name": "code-pros/password-policy",
  "description": "Create a password policy and validate strings against it",
  "type": "library",
  "authors": [
    {
      "name": "John Eckhardt",
      "email": "john@codepros.biz"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1 || ^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "CodePros\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs-check": "phpcs --standard=PSR12 src tests"
  }
}
